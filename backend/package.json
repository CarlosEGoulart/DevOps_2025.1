{
  "name": "galeria-arte",
  "version": "1.0.0",
  "description": "Aplicação de Galeria de Arte com Express, TypeScript e MySQL",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
